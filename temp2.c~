#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <fcntl.h>
#include <errno.h>
#include <sys/types.h>
#include <unistd.h>


int main(int argc, char ** argv){
	char buffer[]="maliha";
	int in_fd=open ("/dev/chardev", O_WRONLY);
	if(in_fd==-1)
	{
		perror("Input file error!");	
		return 1;
	}
	int ret_in=write(in_fd,&buffer,(size_t)strlen(buffer));
	if(ret_in==-1)
	{
		perror("error!");	
		return 1;
	}
	printf("%d\n",ret_in);
	
	return 0;

}
